.button {
  border-radius: 100px;
  background-color: #f6a623;
  color: white;
  text-align: center;
  vertical-align: middle;
  font-family: MicrosoftYaHei;
  width: 560rpx;
  height: 88rpx;
  float: right;
  margin: 0px 32rpx 0px 0px;
  border-style: none;
}

.button2 {
  border-radius: 100px;
  background-color: #5cc017;
  background-image: url("data:image/png;base64,");
  background-size: 40rpx;
  background-repeat: no-repeat;
  background-position: center;
  color: #5cc017;
  width: 88rpx;
  height: 88rpx;
  float: left;
  margin: 0px 0px 0px 32rpx;
}

/*去掉按钮边框*/

button[class="button"]::after {
  border: 0;
}

button[class="button2"]::after {
  border: 0;
}

